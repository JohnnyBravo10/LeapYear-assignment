package dssc.assignment

class Year{
    public int y;

    Year (int a){
        this.y=a;
    }

    public boolean is_leaf(){
        if (this.y%4==0){
            if(this.y%100==0){
                if(this.y%400==0){
                    return true;
                }
                else {
                    return false;
                }
            }
            else{
                return true;
            }

        }
        else{
            return false;
        }
    }
}
